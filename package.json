{
  "name": "tenacious-c",
  "version": "1.0.0",
  "description": "An interactive terminal tool",
  "main": "dist/index.js",
  "bin": {
    "tenacious-c": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "start": "node dist/index.js",
    "dev": "tsc && chmod +x dist/index.js && node dist/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run src",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "typescript": "^5.0.0",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@types/inquirer": "^9.0.9",
    "inquirer": "^13.2.0",
    "ora": "^9.0.0",
    "zod": "^4.3.5"
  }
}
